<% posts.each do |f| %>
  <div class='card w-30 mx-4 my-3'>
    <%= link_to image_tag(f.get_view_image, size:'310x200'), post_path(f.id), 'data-turbolinks': false %>
    <!--post/showでコンソールエラーが出るため'data-turbolinks': falseでリロード表示にする-->
    <div class='card-text p-1'>
      <%= f.title %>
    </div>
  </div>
<% end %>
